{
  "compilerOptions": {
    "target": "es5",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "CommonJS",
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true
  }
}
